<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/dialog_check_in_code_text_type_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:padding="5dp">


    <com.bob.uimodule.views.textviews.MyEditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/dialog_check_in_code_text_type_code_edit_text"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center"
        android:layout_margin="5dp">
    </com.bob.uimodule.views.textviews.MyEditText>


    <RelativeLayout
        android:id="@+id/dialog_check_in_code_text_type_buttons_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_centerHorizontal="true"
        android:layout_margin="5dp"
        android:orientation="horizontal"
        android:layout_below="@+id/dialog_check_in_code_text_type_code_edit_text">

        <com.bob.uimodule.views.textviews.MyButton
            android:id="@+id/dialog_check_in_code_text_type_positive_button"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:layout_alignParentStart="true">
        </com.bob.uimodule.views.textviews.MyButton>

        <com.bob.uimodule.views.textviews.MyButton
            android:id="@+id/dialog_check_in_code_text_type_negative_button"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:layout_toEndOf="@+id/dialog_check_in_code_text_type_positive_button">
        </com.bob.uimodule.views.textviews.MyButton>


    </RelativeLayout>


</RelativeLayout>